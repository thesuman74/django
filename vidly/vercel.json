{
    "builds": [{
      "src": "vidly/vidly/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }],
    "routes": [{
      "src": "/(.*)",
      "dest": "vidly/vidly/wsgi.py"
    }],
    "rewrites": [{
      "source": "/(.*)",
      "destination": "/"
    }]
  }
  